{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "linebreak-style": [2, "unix"],
    "no-underscore-dangle": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "prefer-const": [2],
    "comma-dangle": 0,
    "indent": [2, 2],
    "no-shadow": 0,
    "no-alert": 0,
    "strict": 0
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "env": true
  },
  "plugins": [
  "react"
  ]
}
